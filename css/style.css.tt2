/*Overall Search*/

#search_box_home, #search_box{
    color: #FFF;
    width: 75vw;
    border-radius: 5px;
    border: none;
    float:left;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;

    display:block;
}

#search-submit-go_home, #search-submit-go{
    border:0px;
    margin:0;
    padding:0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    color: #FFF;
    transition: background 0.1s, color 0.15s;
    position:absolute;
}
#search-submit-go_home{
    width: 5%;
    float:right;
}
 #search-submit-go{
    width: 4%;
        background: #EEE;
 }
#search-submit-go_home:hover, #search_box_home:hover + #search-submit-go_home, #search-submit-go:hover, #search-submit-go:active{
    background: #FFF;
    color: var(--MainColor);
    transition: 0.2s;
}  

#searchPusher{
    text-align: center;
    padding-top:15vh;
    width:100%;
    font-family: Verdana, Geneva, sans-serif;
    color: white;
    font-size: 5em;
}



/*Large Search*/

#search_box_home{
    font-size: 5em;
    background: var(--SecondColor);
}

#search-submit-go_home{
